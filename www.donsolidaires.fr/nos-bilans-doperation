      <section id="s_ba2nx61p_v2" data-snippet="s_custom_bilan" style="background-image: none;" class="o_colored_level" data-name="Bloc bilan d’opération zoom">
        <div class="container">
          <div class="grid_container px-5 py-2 bgblanc">
            <div class="bilan_actu_item">
              <div class="custom_card_bilan">
                <div data-original-title="" title="" aria-describedby="tooltip998242">
                  <img src="web/image/238869-8902461f/couverture-etude-hygiene-pour-tous.jpg" alt="" loading="lazy" data-original-title="" title="" aria-describedby="tooltip470279" data-original-id="238689" data-original-src="/web/image/238689-c269c9ba/couverture-etude-hygiene-pour-tous.jpg" data-mimetype="image/jpeg" data-resize-width="690"/>
